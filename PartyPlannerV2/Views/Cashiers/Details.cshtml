@model PartyPlannerV2.Models.Cashier

@{
    ViewData["Title"] = "Cashier Details";
}

<div class="container mt-5">
    <h1>@ViewData["Title"]</h1>

    <div>
        <h4>Cashier</h4>
        <hr />
        <dl class="row">
            <dt class="col-sm-2">Name:</dt>
            <dd class="col-sm-10">@Html.DisplayFor(model => model.Name)</dd>
        </dl>
    </div>

    <div class="mt-3">
        <a asp-action="Edit" asp-route-id="@Model?.CashierId" class="btn btn-primary">Edit</a>
        <a asp-action="Index" class="btn btn-secondary">Back to List</a>
    </div>
</div>